readonly GNU_PLATFORM = "sparc64-openbsd" % "cpu-os" string for GNU

SYSTEM_CC = "g++ -m64 -fPIC -mno-app-regs -gstabs+" % C compiler
SYSTEM_ASM = "as -Qy -s -KPIC -Av9a -64 -no-undeclared-regs -relax" % Assembler

