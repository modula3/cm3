    arg2: MySQLRaw.RefMysqlResultT := LOOPHOLE(mysql_res,
                                               MySQLRaw.RefMysqlResultT);
    result: Int32;
  BEGIN
    result := MySQLRaw.FetchRowCont(arg1, arg2, status);

    IF arg1 = NIL THEN
      ret := NIL;
    ELSE
